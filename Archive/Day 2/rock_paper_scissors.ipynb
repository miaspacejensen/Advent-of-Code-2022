    "def get_data(file_name):\n",
    "    with open(file_name, \"r\") as file:\n",
    "        lines = file.read() \n",
    "    lines = lines.split(\"\\n\")\n",
    "    lines = [line.split(\" \") for line in lines]\n",
    "    return lines"
   ]
